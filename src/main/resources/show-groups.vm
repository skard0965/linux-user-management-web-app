#parse("layout.vm")
#@mainLayout()
<div class="container">
    <h1>Group List</h1>
    <div class="panel panel-default">
        <!-- Table -->
        <table class="table">
            <thead>
            <tr>
                <th>#</th>
                <th>Group</th>
            </tr>
            </thead>
            <tbody>
                #foreach($group in $groups)
                <tr>
                    <th scope="row">$velocityCount</th>
                    <td>$group</td>
                </tr>
                #end
            </tbody>
        </table>
    </div>
</div>
#end